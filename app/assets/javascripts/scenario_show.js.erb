var ready = function() {
  console.log("In ready!")
  var id = $('#scenario').data('id');
  if (id) {
    PrivatePub.subscribe("/scenarios/" + id, function(data, channel) {
      $("#console").append(data.log_message + '<br>');
      $('#console').scrollTop($('#console').prop("scrollHeight"));
    });
  }
  
};

$(document).ready(ready);